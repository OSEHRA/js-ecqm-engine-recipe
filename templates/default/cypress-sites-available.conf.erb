# /etc/apache2/sites-available/cypress
#
# Generated by Chef
#

<VirtualHost *:80>
   PassengerRuby <%= @passenger_ruby %>
   DocumentRoot <%= @cypress_root %>
   TimeOut 1200
   <Directory <%= @cypress_root %>>
      AllowOverride all
      Options -MultiViews
      Require all granted
   </Directory> 
</VirtualHost>